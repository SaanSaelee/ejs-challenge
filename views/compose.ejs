<%- include('partials/header') -%>

<h1>COMPOSE</h1>

<!-- prettier-ignore -->

<form action="/compose" method="POST">
    <div class="form-group">
        <label>Title</label>
        <input class="form-control" type="text" name="postTitle" />
        <label>Post</label>
        <textarea class="form-control" name="postBody" cols="30" rows="5"></textarea>
    </div>
    <button type="submit" class="btn btn-primary" name="button">Publish</button>
</form>

<%- include('partials/footer') -%>
